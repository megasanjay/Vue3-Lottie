{
  "name": "vue3-lottie",
  "version": "2.3.0",
  "keywords": [
    "vue3",
    "lottie"
  ],
  "homepage": "https://vue3-lottie.vercel.app/",
  "bugs": {
    "url": "https://github.com/megasanjay/vue3-lottie/issues",
    "email": "info@sanjaysoundarajan.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/megasanjay/vue3-lottie.git"
  },
  "license": "MIT",
  "author": "Sanjay Soundarajan <info@sanjaysoundarajan.dev> (https://sanjaysoundarajan.dev)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/vue3-lottie.js",
      "require": "./dist/vue3-lottie.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "dist/vue3-lottie.cjs",
  "module": "dist/vue3-lottie.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "dev": "vite",
    "lint": "eslint \"{packages,playground}/**/*.{ts,tsx,vue,js,jsx,html}\"",
    "prepublishOnly": "npm run build",
    "prettier": "npx prettier --write ."
  },
  "dependencies": {
    "lottie-web": "^5.9.6"
  },
  "devDependencies": {
    "@babel/types": "~7.19.3",
    "@types/node": "~18.8.3",
    "@vitejs/plugin-vue": "~3.1.2",
    "@vue/compiler-sfc": "~3.2.40",
    "typescript": "~4.8.4",
    "vite": "~3.1.6",
    "vite-plugin-css-injected-by-js": "~2.1.0",
    "vite-plugin-vue-type-imports": "~0.2.3",
    "vue": "~3.2.40",
    "vue-tsc": "~0.40.13"
  },
  "peerDependencies": {
    "vue": "^3.2"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
